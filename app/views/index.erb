<header>
  <h1>SlackOverflow</h1>
  <p id="tabs">
    <a href="#posts">latest</a>
    <a href="#">comments</a>
    <a href="#">popular</a>
    <a href="#posts">add new question</a>
  </p>
</header>

<h2>All Questions</h2>
<ul>
	<% @questions.each do |question| %>
		<li class="question-listing"><a class="question-hyperlink" href="/questions/<%= question.id %>"><%= question.title %></a></li>
	<% end %>
</ul>


<form id="posts" method="post" action="/posts">
  <input type="text" name="title" placeholder="title">
  <input type="submit" value="submit new question">
</form>
